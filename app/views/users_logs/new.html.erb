<h1>New users_log</h1>

<% form_for(@users_log) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :access_date %><br />
    <%= f.date_select :access_date %>
  </p>
  <p>
    <%= f.label :time_using %><br />
    <%= f.time_select :time_using %>
  </p>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :project_id %><br />
    <%= f.text_field :project_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', users_logs_path %>