<h1>New jumper</h1>

<% form_for(@jumper) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :connectors %><br />
    <%= f.text_field :connectors %>
  </p>
  <p>
    <%= f.label :lenght %><br />
    <%= f.text_field :lenght %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :fiber_id %><br />
    <%= f.text_field :fiber_id %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', jumpers_path %>