<h1>Listing sites_additional_informations</h1>

<table>
  <tr>
    <th>Cable type</th>
    <th>Fiber type</th>
    <th>Cable manufacturer</th>
    <th>Cable manufacturing date</th>
    <th>Installer company</th>
    <th>Connectors qty</th>
    <th>Splices qty</th>
  </tr>

<% @sites_additional_informations.each do |sites_additional_information| %>
  <tr>
    <td><%=h sites_additional_information.cable_type %></td>
    <td><%=h sites_additional_information.fiber_type %></td>
    <td><%=h sites_additional_information.cable_manufacturer %></td>
    <td><%=h sites_additional_information.cable_manufacturing_date %></td>
    <td><%=h sites_additional_information.installer_company %></td>
    <td><%=h sites_additional_information.connectors_qty %></td>
    <td><%=h sites_additional_information.splices_qty %></td>
    <td><%= link_to 'Show', sites_additional_information %></td>
    <td><%= link_to 'Edit', edit_sites_additional_information_path(sites_additional_information) %></td>
    <td><%= link_to 'Destroy', sites_additional_information, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

